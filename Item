import QtQuick 2.12
import QtQuick.Window 2.12

Window{
    id: mywindow
    width: 800
    height: 800
    visible: true
    
   Item{                          // Item is a class and it has no visual appearance
       id: myitem 
       anchors.centerIn: parent 
       width : 200
       height: 200
       
       Rectangle {
           id: myrect
           anchors.fill: parent       // it basically takes all the item of parent. Here it takes width,height and anchor.center
           color: "red"
       }
   }
}
